<%= simple_form_for [group] do |f| %>
  <%= f.input :name, label: "Nom du groupe" %>
  <%= f.input :start_date, label: "Dates du projet", as: :string, required: false, input_html: {id: "range_start", class: "d-none"} %>
  <%= f.input :end_date, label: false, as: :string, required: false, input_html: {id: "range_end", class: "d-none"}  %>
  <%= f.submit label%>
<% end %>
